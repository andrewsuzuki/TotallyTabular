
<faust>
	<name>GreyholeRaw</name>
	<author>Julian Parker</author>
	<copyright>(c) Julian Parker 2013</copyright>
	<license>none</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>2</inputs>
	<outputs>2</outputs>
	<ui>
		<activewidgets>
			<count>7</count>
			<widget type="hslider" id="1">
				<label>damping</label>
				<varname>fslider2</varname>
				<init>0.0f</init>
				<min>0.0f</min>
				<max>0.99f</max>
				<step>0.001f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>delayTime</label>
				<varname>fslider5</varname>
				<init>0.2f</init>
				<min>0.001f</min>
				<max>1.45f</max>
				<step>0.0001f</step>
			</widget>
			<widget type="hslider" id="3">
				<label>diffusion</label>
				<varname>fslider1</varname>
				<init>0.5f</init>
				<min>0.0f</min>
				<max>0.999f</max>
				<step>0.0001f</step>
			</widget>
			<widget type="hslider" id="4">
				<label>feedback</label>
				<varname>fslider6</varname>
				<init>0.9f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="5">
				<label>modDepth</label>
				<varname>fslider4</varname>
				<init>0.1f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.001f</step>
			</widget>
			<widget type="hslider" id="6">
				<label>modFreq</label>
				<varname>fslider3</varname>
				<init>2.0f</init>
				<min>0.0f</min>
				<max>1e+01f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="7">
				<label>size</label>
				<varname>fslider0</varname>
				<init>1.0f</init>
				<min>0.5f</min>
				<max>3.0f</max>
				<step>0.0001f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>GreyholeRaw</label>
				<widgetref id="1" />
				<widgetref id="2" />
				<widgetref id="3" />
				<widgetref id="4" />
				<widgetref id="5" />
				<widgetref id="6" />
				<widgetref id="7" />
			</group>
		</layout>
	</ui>
</faust>
